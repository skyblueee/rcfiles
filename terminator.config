[global_config]
  borderless = True
  title_hide_sizetext = True
  suppress_multiple_term_dialog = True
[keybindings]
  cycle_next = None
  cycle_prev = None
  go_next = None
  go_prev = None
  close_term = None
  close_window = None
  hide_window = None
  group_all = None
  group_tab = None
  broadcast_off = None
  broadcast_group = None
  broadcast_all = None
  insert_number = None
  insert_padded = None
  edit_window_title = None
  edit_tab_title = None
  edit_terminal_title = None
  help = None
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    show_titlebar = False
    scrollback_lines = 5000
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
