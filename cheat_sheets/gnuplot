#!/usr/bin/gnuplot -persit
set xrange [0:8]
set yrange [0:3.5]
unset xtics
unset ytics
unset key
unset border

# rectangle
set object 1 rect from x1,y1 to x2,y2 fc '#EEEEEE'

# line
set arrow 1 from x1,y1 to x2,y2 nohead dashtype "._-"

# arrow
set arrow 5 from x1,y1 to x2,y2 heads filled size screen 0.03,15,45

# text/label
set label "IoU = I/U" at x,y center font "Times, 16"

# invisable line y=0
plot 0 lt bgnd
